{
  "name": "school-api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "dev:debug": "nodemon --inspect",
    "start": "node --loader ts-node/esm src/server.ts",
    "build": "npx tsc",
    "serve": "node dist/server.js",
    "dev-build": "npm run build && npm run serve",
    "prisma:setup": "npx prisma generate && npx prisma migrate dev",
    "seed": "npx ts-node --esm prisma/seed.ts",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:generate": "npx prisma generate",
    "clean": "rm -rf dist node_modules/.cache",
    "install:all": "npm install && cd Client/my-project && npm install && cd ..",
    "dev:full": "npm run dev & cd Client/my-project && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
