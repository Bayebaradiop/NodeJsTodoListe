# Configuration de la base de données MySQL
# Remplacez 'your_password' par votre mot de passe MySQL
DATABASE_URL="mysql://root:your_password@localhost:3306/mydb1"

# Clé secrète pour JWT (générez une clé forte et unique)
# Vous pouvez utiliser: openssl rand -base64 32
JWT_SECRET="your_super_secret_jwt_key_here_minimum_32_characters"

# Port du serveur (optionnel, défaut: 3000)
PORT=3000

# Environnement (optionnel, automatiquement défini en développement)
NODE_ENV=development